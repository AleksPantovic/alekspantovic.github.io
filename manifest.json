{
  "manifestVersion": "2.0.0",
  "pluginVersion": "0.1.0",
  "name": {
    "en": "Asioso Haiilo Plugin",
    "de": "Asioso Haiilo Plugin"
  },
  "description": {
    "en": "Integration plugin for Haiilo platform",
    "de": "Integrationsplugin f√ºr die Haiilo-Plattform"
  },
  "entryPoints": [
    {
      "id": "510e9e0c-e3b8-44a8-8db1-5185d3d844ce",
      "url": "/",
      "name": {
        "en": "User Management",
        "de": "Benutzerverwaltung"
      },
      "description": {
        "en": "Manage Haiilo user data",
        "de": "Haiilo-Benutzerdaten verwalten"
      }
    }
  ],
  "lifecycle": {
    "url": "/.netlify/functions/lifecycle",
    "events": [
      "install",
      "uninstall",
      "instance_add",
      "instance_remove"
    ]
  },
  "webhooks": {
    "access_token": "https://haiiloplugin.netlify.app/.netlify/functions/oauth-callback"
  },
  "oauth2": {
    "scopes": [
      "plugin:notify",
      "users.read"
    ],
    "client_id": "organization"
  },
  "apiAccess": {
    "scopes": ["users.read"],
    "endpoints": ["/api/users"]
  }
}
